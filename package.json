{
  "name": "nahsdirt",
  "version": "1.0.0",
  "private": true,
  "author": "Jacob Cohen <jacob@yakovliam.com> (@yakovliam)",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start -p 3002",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@types/memory-cache": "0.2.2",
    "@types/mongo-sanitize": "1.0.1",
    "@types/uuid": "8.3.4",
    "axios": "0.26.1",
    "grommet": "2.21.0",
    "grommet-icons": "4.7.0",
    "lru-cache": "7.5.1",
    "memory-cache": "0.2.0",
    "mongo-sanitize": "1.1.0",
    "mongoose": "6.2.7",
    "next": "12.1.0",
    "node-sass": "7.0.1",
    "polished": "4.1.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "recoil": "0.6.1",
    "recoil-persist": "4.0.0",
    "rss-parser": "3.12.0",
    "styled-components": "5.3.3",
    "swr": "1.2.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@types/node": "16.11.26",
    "@types/react": "17.0.40",
    "@types/react-dom": "17.0.13",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.5",
    "prettier": "2.5.1",
    "typescript": "4.6.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
